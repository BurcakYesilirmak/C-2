/*finding maximum and the minimum numbers of an array*/
/*ayrı ayrı for koymalı mıyım?*/
#include<stdio.h>
int main()
{
int i,max,min;
int k[]={10,123,133,32,123,-283,27,23,3,23};
max=k[1];
min=k[1];
for(i=0;i<10;i++) {
 if(k[i]>max ) max=k[i];
 }
for(i=0;i<10;i++) {
 if(k[i]<min ) min=k[i];
 }
printf("max=%d\n",max);

printf("min=%d\n",min);
return 0;
}
 
 
 
