#include<stdio.h>
//preinc-predec add or sub 1 first to var,yields the new value of var
//postinc-postdec the original value of var is assgned, then 1 add or sub
int main()
{
int w,x,y,z,result;
w=x=y=z=1;
printf("given w=%d x=%d y=%d z=%d\n",w,x,y,z);
result=++w;
printf("++w evaluates to %d and w is now %d\n",result,w);
result=x++;
printf("x++ evaluates to %d and x is now %d\n",result,x);
result=--y;
printf("--y evaluates to %d and y is now %d\n",result,y);
result=z--;
printf("z-- evaluates to %d and z is now %d\n",result,z);
return 0;
}
