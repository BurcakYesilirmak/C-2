int main()
{
int x[2][3]={1,2,3,4,5,6};
int y[2][3]={2,3,4,3,3,3};
int c[2][3];
int i,j;
printf("A matrisi\n");
for(i=0;i<2;i++) {
 for(j=0;j<3;j++)
  printf("%3d",x[i][j]);
 printf("\n");
}
printf("B matrisi:\n");
for(i=0;i<2;i++) {
 for(j=0;j<3;j++) 
  printf("%3d",y[i][j]);
 printf("\n");
}
printf("A+B=C\n");
for(i=0;i<2;i++) {
 for(j=0;j<3;j++){
  c[i][j]=x[i][j]+y[i][j];
  printf(" %3d",c[i][j]);
 }
 printf("\n");
 }
return 0;
}
